<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--分享控件拓展功能，替换图片，隐藏短信，文字颜色，目前支持这三种-->
    <declare-styleable name="ShareView">
        <attr name="weixin_icon" format="reference"/>
        <attr name="qq_icon" format="reference"/>
        <attr name="msg_icon" format="reference"/>

        <attr name="weixin_tx" format="string|reference"/>
        <attr name="qq_tx" format="string|reference"/>
        <attr name="msg_tx" format="string|reference"/>

        <attr name="share_tx_color" format="color|reference"/>

        <attr name="need_mis_msg" format="boolean"/>

    </declare-styleable>

</resources>